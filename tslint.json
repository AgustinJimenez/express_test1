{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [ false ],
        "semicolon": [ false ],
        "one-line": [ false ],
        "no-trailing-whitespace": [ false ],
        "variable-name": [ false ],
        "indent": [ true,  "spaces"],
        "new-parens": false,
        "member-access": false,
        "no-consecutive-blank-lines": false,
        "eofline": false,
        "curly": false,
        "ordered-imports": false,
        "no-var-requires": false,
        "object-literal-sort-keys": false,
        "no-console": false
        
    },
    "rulesDirectory": []
}